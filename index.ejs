		<div id="refresh">
			<a id="re" href="/"><i class="fa fa-repeat" aria-hidden="true"></i></a>
		</div>
		
        
        <% for (var i = id.length - 1; i > 0; i--) {%>
         <div class="transcript bigbluefish">
            <h1><span><%= id[i].date%></span></h1>
            <p><span><%= id[i].transcript%></span></p>
        </div>
        <%} %>

		
	</body>
